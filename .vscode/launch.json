{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/MutagenMerger.CLI/bin/Debug/net6.0/MutagenMerger.CLI.dll",
      "args": [
        "--game",
        "SkyrimSE",
        "--mergefile",
        "SkyrimMerge.txt",
        "--data",
        "/mnt/mediaSSD/SteamLibrary/steamapps/common/Skyrim Special Edition/Data",
        "--output",
        "/home/monyarm/Downloads/TestMerge",
        "--mergename",
        "TestMerge.esp"
      ],
      "cwd": "${workspaceFolder}/MutagenMerger.CLI",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
